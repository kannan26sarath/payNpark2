<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kanna.paynpark">

    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.Manifest.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ll"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomePage" />
        <activity
            android:name=".LoginActivity"
            android:theme="@style/NoActionBar"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".ParkActivity"
            android:parentActivityName=".HomePage" />
        <activity
            android:name=".SearchActivity"
            android:parentActivityName=".HomePage" />
        <activity
            android:name=".LocateActivity"
            android:parentActivityName=".HomePage" />
        <activity
            android:name=".BillingActivity"
            android:parentActivityName=".HomePage" />
        <activity
            android:name=".AdminActivity"
            android:parentActivityName=".HomePage" />
        <activity
            android:name=".AboutActivity"
            android:parentActivityName=".HomePage"
            android:theme="@style/NoActionBar" />
        <activity android:name=".SlotActivity" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/NoActionBar" />
        <activity android:name=".ChangepassActivity" />
        <activity android:name=".FareupActivity" />
        <activity android:name=".RevenueActivity"></activity>
    </application>

</manifest>